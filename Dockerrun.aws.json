{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitios": [
        {
            "name":"client",
            "image": "davidavilab/multi-client",
            "hostname":"client",
            "essential": false            
        },
        {
            "name":"server",
            "image": "davidavilab/multi-server",
            "hostname":"api",
            "essential": false            
        },
        {
            "name":"worker",
            "image": "davidavilab/multi-worker",
            "hostname":"worker",
            "essential": false            
        }
        ,
        {
            "name":"nginx",
            "image": "davidavilab/multi-nginx",
            "essential": true,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links":["client","server"]            
        }
    ]
}